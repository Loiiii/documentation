{% if contributor_list %}
    {% for contributorhandle in contributor_list %}
        {% if site.contributors[contributorhandle] %}
            {% set contributor = site.contributors[contributorhandle] %}
        {% endif %}
        {% if not loop.last %}
            <a href="{{ site.url }}/contributors/{{ contributorhandle }}" title="{{ contributorhandle }}">{{ contributor.name }}</a>,
        {% else %}
            <a href="{{ site.url }}/contributors/{{ contributorhandle }}" title="{{ contributorhandle }}">{{ contributor.name }}</a>
        {% endif %}
    {% endfor %}
{% endif %}
